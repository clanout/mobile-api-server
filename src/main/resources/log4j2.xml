<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>

        <Console name="StdOut" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} (%t) %m%n"/>
        </Console>

    </Appenders>

    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="StdOut" level="DEBUG"/>
        </Root>
    </Loggers>

</Configuration>
